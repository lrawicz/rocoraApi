{
  "name": "rocora-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrationUp": "typeorm-ts-node-commonjs migration:run -d ./src/dataSource.ts",
    "migrationDown": "typeorm-ts-node-commonjs migration:revert -d ./src/dataSource.ts",
    "migrationGenerate": "typeorm-ts-node-commonjs migration:create",
    "seed": "typeorm-ts-node-commonjs seed:run -d ./src/dataSource.ts",
    "swagger": "swagger-jsdoc -d ./src/swagger.json -o ./docs/swagger.json",
    "swagger-ui": "swagger-ui-express -d ./docs/swagger.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
